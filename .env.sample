# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USERNAME=your_username
DB_PASSWORD=your_password

# Application Configuration
SPRING_PROFILES_ACTIVE=dev

# Keycloak Configuration
KEYCLOAK_ISSUER_PREFIX=https://keycloak.yourdomain.com/realms/
KEYCLOAK_SAAS_ISSUER_URI=https://auth.company.com/realms/saas-admin

# OpenTelemetry / SigNoz Configuration
SIGNOZ_ENDPOINT=ingest.us.signoz.cloud:443
SIGNOZ_ACCESS_TOKEN=your_signoz_ingestion_key
OTEL_COLLECTOR_ENDPOINT=http://localhost:4318

# Frontend Deployment (Vite build-time variables)
VITE_API_BASE_URL=https://api.forge.keeplearningos.com
VITE_KEYCLOAK_URL=https://keycloak.keeplearningos.com
VITE_KEYCLOAK_REALM=saas-admin
VITE_KEYCLOAK_CLIENT_ID=kos-admin-web
VITE_OTEL_COLLECTOR_URL=https://otel.keeplearningos.com
VITE_OTEL_ENABLED=true

# Gradle S3 Build Cache (optional - speeds up builds by sharing cache)
# Works with AWS S3 or S3-compatible storage (MinIO, Cloudflare R2, Backblaze B2)
S3_BUILD_CACHE_BUCKET=gradle-cache
S3_BUILD_CACHE_REGION=auto
# Use standard AWS SDK credential names
AWS_ACCESS_KEY_ID=your_r2_access_key
AWS_SECRET_ACCESS_KEY=your_r2_secret_key
# For Cloudflare R2 (required):
S3_BUILD_CACHE_ENDPOINT=https://YOUR_ACCOUNT_ID.r2.cloudflarestorage.com
